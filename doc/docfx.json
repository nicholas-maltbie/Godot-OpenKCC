{
    "build": {
      "template": [
        "default",
        "modern",
        "godot-template"
      ],
      "content": [
        {
          "files": ["*.yml"],
          "src": "gen/api",
          "dest": "api"
        },
        {
          "files": ["*.yml"],
          "src": "gen/example",
          "dest": "example"
        },
        {
          "files": ["*.yml", "*.md"],
          "src": "design",
          "dest": "design"
        },
        {
          "files": [
            "toc.yml",
            "*.md"
          ]
        }
      ],
      "resource": ["**/*.png"],
      "xref": [
        "https://gddoc2yml.nickmaltbie.com/xrefmap.yml"
      ],
      "dest": "_site"
    }
  }